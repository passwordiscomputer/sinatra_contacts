<h1><b>Name</b></h1>
<h2><%="#{@contact.first_name} #{@contact.last_name}"%></h2>
<h1><b>Job Title</b></h1>
<h2><%="#{@contact.job_title}"%></h2>
<h1><b>Company</b></h1>
<h2><%="#{@contact.company}"%></h2>
<h1><b>Contact Type</b></h1>
<h2><%="#{@contact.contact_type}"%></h2>

<form action="/contacts/:first_name" method="post">
  <div class="form-group">
    <label for="street">Street</label>
    <input id="street" name="street" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="rank">City</label>
    <input id="city" name="city" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="state">State</label>
    <input id="state" name="state" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="zip">Zip Code</label>
    <input id="zip" name="zip" class="form-control" type="text">
  </div>
  <button type="submit" class="btn btn-primary">Go!</button>
</form>
